<ip_presets schema="1.0">
	<ip_preset preset_proc_name="ps_pmc_fixed_io_preset">
		<ip vendor="xilinx.com" library="ip" name="versal_cips" version="*" ip_interface="FIXED_IO">
			<user_parameters>
				<user_hier_parameter name="CONFIG.PS_PMC_CONFIG">

					<!-- They are just input bit to a flash memory -->
					<!-- <user_hier_parameter name="PS_MIO7">
						<user_parameter name="AUX_IO" value="0"/>
						<user_parameter name="OUTPUT_DATA" value="high"/>
						<user_parameter name="PULL" value="disable"/>
					</user_hier_parameter>
					<user_hier_parameter name="PS_MIO9">
						<user_parameter name="AUX_IO" value="0"/>
						<user_parameter name="OUTPUT_DATA" value="high"/>
						<user_parameter name="PULL" value="disable"/>
					</user_hier_parameter> -->

					<!-- CAN Inhibit functionality not present -->
					<!-- <user_hier_parameter name="PMC_MIO37">
						<user_parameter name="AUX_IO" value="0"/>
						<user_parameter name="DIRECTION" value="out"/>
						<user_parameter name="OUTPUT_DATA" value="high"/>
						<user_parameter name="USAGE" value="GPIO"/>
					</user_hier_parameter> -->

					<user_hier_parameter name="PMC_MIO12">
						<user_parameter name="AUX_IO" value="0"/>
						<user_parameter name="DIRECTION" value="out"/>
						<!-- <user_parameter name="OUTPUT_DATA" value="high"/> -->
						<user_parameter name="USAGE" value="GPIO"/>
					</user_hier_parameter>	
					
					<user_hier_parameter name="PMC_SD1">
						<user_parameter name="CD_ENABLE" value="1"/>
						<user_parameter name="POW_ENABLE" value="1"/>
						<user_parameter name="RESET_ENABLE" value="0"/>
						<user_parameter name="WP_ENABLE" value="0"/>
					</user_hier_parameter>


					<!-- <user_parameter name="PMC_SD1_COHERENCY" value="0"/> -->
					<!-- <user_parameter name="PMC_SD1_DATA_TRANSFER_MODE" value="8Bit"/> -->
					<user_hier_parameter name="PMC_SD1_PERIPHERAL">
						<user_parameter name="ENABLE" value="1"/>
						<user_hier_parameter name="IO">
							<user_parameter name="PMC_MIO" value="38 .. 45"/>
						</user_hier_parameter>
					</user_hier_parameter>
					<user_hier_parameter name="PMC_SD1_SLOT_TYPE">
						<user_parameter name="SD" value="3.0"/>
					</user_hier_parameter>
					
					<!-- BANK 500 (OSPI, USB) -->
					<user_hier_parameter name="PMC_GPIO0_MIO_PERIPHERAL">
						<user_parameter name="ENABLE" value="1"/>
						<user_hier_parameter name="IO">
							<user_parameter name="PMC_MIO" value="0 .. 25"/>
						</user_hier_parameter>
					</user_hier_parameter>
					
					<!-- BANK 501 (EMMC, PBs, LEDs, SD, I2C)-->
					<user_hier_parameter name="PMC_GPIO1_MIO_PERIPHERAL">
						<user_parameter name="ENABLE" value="1"/>
						<user_hier_parameter name="IO">
							<user_parameter name="PMC_MIO" value="26 .. 51"/>
						</user_hier_parameter>
					</user_hier_parameter>
					
					<!-- I2C -->
					<user_hier_parameter name="PS_I2C0_PERIPHERAL">
						<user_parameter name="ENABLE" value="1"/>
						<user_hier_parameter name="IO">
							<user_parameter name="PMC_MIO" value="50 .. 51"/>
						</user_hier_parameter>
					</user_hier_parameter>
					
					<!-- OSPI -->
					<user_hier_parameter name="PMC_OSPI_PERIPHERAL">
						<user_parameter name="ENABLE" value="1"/>
						<user_hier_parameter name="IO">
							<user_parameter name="PMC_MIO" value="0 .. 11"/>
						</user_hier_parameter>
						<user_parameter name="MODE" value="Single"/>
					</user_hier_parameter>

					<!-- EMMC (NOTE PMC_MIO28 -> MIO28_I2C_EXP_RST_B) -->
					<user_hier_parameter name="PMC_EMMC_PERIPHERAL">
						<user_parameter name="ENABLE" value="1"/>
						<user_hier_parameter name="IO">
							<user_parameter name="PMC_MIO" value="26 .. 36"/>
						</user_hier_parameter>
						<user_parameter name="MODE" value="Single"/>
					</user_hier_parameter>
					<!-- <user_parameter name="PMC_QSPI_COHERENCY" value="0"/> -->
					<!-- <user_hier_parameter name="PMC_QSPI_FBCLK"> -->
					<!-- <user_parameter name="ENABLE" value="1"/> -->
					<!-- <user_hier_parameter name="IO"> -->
					<!-- <user_parameter name="PMC_MIO" value="6"/> -->
					<!-- </user_hier_parameter> -->
					<!-- </user_hier_parameter> -->
					<!-- <user_parameter name="PMC_QSPI_PERIPHERAL_DATA_MODE" value="x4"/> -->
					<!-- <user_parameter name="PMC_QSPI_PERIPHERAL_ENABLE" value="1"/> -->
					<!-- <user_hier_parameter name="PMC_QSPI_PERIPHERAL_MODE"> -->
					<!-- <user_parameter name="Dual" value="Parallel"/> -->
					<!-- </user_hier_parameter> -->
					<!-- <user_parameter name="PS_CAN1_PERIPHERAL"> -->
					<!-- <user_parameter name="ENABLE" value="1"/> -->
					<!-- <user_parameter name="IO"> -->
					<!-- <user_parameter name="PMC_MIO" value="40 .. 41"/> -->
					<!-- </user_parameter> -->
					<!-- </user_parameter> -->

					<!-- GIGABIT ETHERNET -->
					<user_hier_parameter name="PS_ENET0_MDIO">
						<user_parameter name="ENABLE" value="1"/>
						<user_hier_parameter name="IO">
							<user_parameter name="PS_MIO" value="24 .. 25"/>
						</user_hier_parameter>
					</user_hier_parameter>
					<user_hier_parameter name="PS_ENET0_PERIPHERAL">
						<user_parameter name="ENABLE" value="1"/>
						<user_hier_parameter name="IO">
							<user_parameter name="PS_MIO" value="0 .. 11"/>
						</user_hier_parameter>
					</user_hier_parameter>
					<!-- <user_parameter name="PS_ENET1_PERIPHERAL"> -->
					<!-- <user_parameter name="ENABLE" value="1"/> -->
					<!-- <user_parameter name="IO"> -->
					<!-- <user_parameter name="PS_MIO" value="12 .. 23"/> -->
					<!-- </user_parameter> -->
					<!-- </user_parameter> -->
					
					<!-- UART -->
					<user_hier_parameter name="PS_UART0_PERIPHERAL">
						<user_parameter name="ENABLE" value="1"/>
						<user_hier_parameter name="IO">
							<user_parameter name="PMC_MIO" value="16 .. 17"/>
						</user_hier_parameter>
					</user_hier_parameter>

					<!-- USB -->
					<user_hier_parameter name="PS_USB3_PERIPHERAL">
						<user_parameter name="ENABLE" value="1"/>
						<user_hier_parameter name="IO">
							<user_parameter name="PMC_MIO" value="13 .. 25"/>
						</user_hier_parameter>
					</user_hier_parameter>
					
				</user_hier_parameter>
			</user_parameters>
		</ip>
	</ip_preset>
	
	<ip_preset preset_proc_name="LPDDR4_Controller0_preset">
		<ip vendor="xilinx.com" library="ip" name="axi_noc" version="*">
			<user_parameters>
				<user_parameter name="CONFIG.CONTROLLERTYPE" value="LPDDR4_SDRAM" />
				<user_parameter name="CONFIG.MC_NO_CHANNELS" value="Dual" />
				<user_parameter name="CONFIG.MC_SYSTEM_CLOCK" value="Differential" />
				<user_parameter name="CONFIG.MC_MEMORY_SPEEDGRADE" value="LPDDR4-3733" />
				<user_parameter name="CONFIG.MC_DATAWIDTH" value="32" />
				<user_parameter name="CONFIG.MC_LP4_PIN_EFFICIENT " value="true" />
				<user_parameter name="CONFIG.MC_FREQ_SEL" value="MEMORY_CLK_FROM_SYS_CLK" />
				<user_parameter name="CONFIG.MC_IP_TIMEPERIOD0_FOR_OP" value="5000" />
				<user_parameter name="CONFIG.MC_OP_TIMEPERIOD0" value="541" />
				<user_parameter name="CONFIG.MC_LP4_OVERWRITE_IO_PROP" value="true" />
			</user_parameters>
		</ip>
	</ip_preset>
	
	<ip_preset preset_proc_name="sysclk0_preset"> 	<!-- N23, N24 (200 MHz)-->
		<ip vendor="xilinx.com" library="ip" name="clk_wiz" ip_interface="CLK_IN1_D">
			<user_parameters>
				<user_parameter name="CONFIG.PRIM_IN_FREQ" value="200" />
				<user_parameter name="CONFIG.PRIM_SOURCE" value="Differential_clock_capable_pin" />
			</user_parameters>
		</ip>
		<ip vendor="xilinx.com" library="ip" name="util_ds_buf">
			<user_parameters>
				<user_parameter name="CONFIG.C_BUF_TYPE" value="IBUFDS" />
				<user_parameter name="CONFIG.C_SIZE" value="1" />
			</user_parameters>
		</ip>
	</ip_preset>
	
	<!-- Some are PMC so I don't know if I should put them in the "ps_pmc_fixed_io_preset".
		xpio and gpio are physically the same (see package files and the doc:AM013) -->
	<!-- Push Buttons -->
	<ip_preset preset_proc_name="pmc_pb_preset">
		<ip vendor="xilinx.com" library="ip" name="axi_gpio" ip_interface="GPIO">
